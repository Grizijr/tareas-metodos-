   "source": [
    "#datos entregados\n",
    "distancia = 2 #metros\n",
    "tiempo = 0.3 #segundos \n",
    "gravedad = 9.8 #metros/segundos^2\n",
    "\n",
    "#formula de velocidad inicial\n",
    "v0 = (distancia - (1/2) * gravedad * tiempo**2) / tiempo\n",
    "\n",
    "#resultado\n",
    "print(\"velocidad inicial:\", v0,\"m/s\")\n"
   ]
